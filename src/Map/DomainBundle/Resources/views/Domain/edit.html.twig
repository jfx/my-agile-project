{% extends 'MapCoreBundle::layout.html.twig' %}

{% block title %}{{ parent() }} - Domain edit - Id : {{ domain.id }}{% endblock title %}

{% block headline %}{{ breadcrumb(['Admin', 'Edit domain']) }}{% endblock headline %}

{% block content %}
<div class="row-fluid">
    <ul class="nav nav-tabs" role="tablist">
        <li class="active"><a id="a_main" href="#">Main</a></li>
        <li><a id="a_resources" href="{{ path('dm-resource_index') }}">Resources</a></li>
    </ul>
</div>
<br />
<div class="row">
    <div class="col-md-6 col-md-offset-3">

        {% include "MapCoreBundle::form.html.twig" %}

    </div>
</div>
<div class="row-fluid">
    <a class="btn btn-default btn-sm" href="{{ path('domain_index') }}"
       title="Return to list">
        {{ icon('chevron-left') }} Return to list
    </a>
</div>
{% endblock content %}
